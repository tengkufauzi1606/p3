// Fungsi untuk membuat elemen gambar dan menampilkannya
function displayImage(imageUrl) {
    // Buat elemen gambar
    var imgElement = document.createElement('img');
    
    // Atur sumber gambar
    imgElement.src = imageUrl;
    
    // Atur atribut lain jika diperlukan
    // imgElement.alt = 'Alternative Text'; // Jika Anda ingin menambahkan teks alternatif
    
    // Tambahkan elemen gambar ke dalam container
    var container = document.getElementById('imageContainer');
    container.appendChild(imgElement);
}

// Panggil fungsi displayImage dengan URL gambar
var imageUrl = 'https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEgx15ZsT-n-e5E-QMwQDanQNHN0f87vblUSPe7pHN80bX5XEPKUMJjfZJPyWhNV1xfGF_qp5G0HP3hQqpeDHLOST4wTepxgBmUao1NiuZtFsSDZFXkuqRTuTIMAsrrBAsEnmpMiT5AAyqISJ2o45lWtRk_chSh7K0GD_9iLAWtBwc4CcrcvBng7-3DIfC4/w640-h534/recoverydeletedphotos.gif';
displayImage(imageUrl);
